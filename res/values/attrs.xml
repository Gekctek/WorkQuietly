<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="ChangeableAnalogClock">
        <attr name="dial" format="reference"/>
        <attr name="hand_hour" format="reference"/>
        <attr name="hand_minute" format="reference"/>
    </declare-styleable>
    <declare-styleable name="RecurringTextView">
        <attr name="recurring_start_time" format="reference"/>
        <attr name="recurring_end_time" format="reference" />
        <attr name="recurring_days_of_week" format="reference"/>
        <attr name="recurring_name" format="reference" />
        <attr name="recurring_enabled" format="reference" />
    </declare-styleable>
    <declare-styleable name="OneTimeTextView">
        <attr name="one_time_start_time" format="reference"/>
        <attr name="one_time_end_time" format="reference" />
        <attr name="one_time_name" format="reference" />
        <attr name="one_time_enabled" format="reference" />
    </declare-styleable>
    <declare-styleable name="CalendarTextView">
        <attr name="calendar_name" format="reference" />
        <attr name="calendar_filter_text" format="reference" />
        <attr name="calendar_enabled" format="reference" />
    </declare-styleable>
    <declare-styleable name="GPSTextView">
        <attr name="gps_name" format="reference" />
        <attr name="gps_radius" format="reference" />
        <attr name="gps_latitude" format="reference" />
        <attr name="gps_longitude" format="reference" />
        <attr name="gps_enabled" format="reference" />
    </declare-styleable>
    <string-array name="gps_power_values">
        <item >3</item>
        <item >2</item>
        <item >1</item>
    </string-array>
    <string-array name="gps_power_names">
        <item >High</item>
        <item >Medium</item>
        <item >Low</item>
    </string-array>
    </resources>
